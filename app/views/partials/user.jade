#profileContainer.shadow( page-data="userData"  )
    div
      #userName
        a(href='javascript:changeProfileColor()') {{userData.username}}
        #profileClicks(style='color: white; font-size: 12px;') 10484 clicks
      div#userImage()
        img#profileImage(src='{{pageData.userImage}}')
        .userEdit.nuMenu(ng-click='$parent.addMenu=!$parent.addMenu' ng-show='currentUser.name==userProfile')
          .fa.fa-pencil

div.pageList.userList( ng-include="'/partials/userGallery'", infinite)

