  .row(ng-controller="LoginCtrl")
    .col-sm-12
      h1 Login
        
    .col-sm-12
      form.form(name='form', ng-submit='login(form)', novalidate='novalidate')
        .form-group
          label Username
          input.textInput(type='text', name='email', ng-model='user.email')
        .form-group
          label Password
          input.textInput(type='password', name='password', ng-model='user.password')
        .form-group.has-error
          p.help-block(ng-show='form.email.$error.required && form.password.$error.required && submitted')
            | Please enter your email and password.
          p.help-block {{ errors.other }}
        button.textInput(type='submit')  Sign in 
        span.clearfix
  .row
    .col-sm-12
